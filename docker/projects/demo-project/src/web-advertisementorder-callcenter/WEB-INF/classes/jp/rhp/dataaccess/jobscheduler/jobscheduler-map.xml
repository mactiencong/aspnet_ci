<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="jp.rhp.dataaccess.jobscheduler.JobSchedulerDataAccess">

	<select id="findCompanyId" resultClass="java.lang.Integer">
		SELECT
			employer_id AS company_id
		FROM
			employer.employer
	</select>

</sqlMap>
